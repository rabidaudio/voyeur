{
  "name": "Voyeur - image manager",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A image viewer using curiosadb.com",
  "permissions": [
    {
        "mediaGalleries": ["read", "allAutoDetected"]
    },
    "fullscreen"
  ],
  "icons": {
    "128": "mga-128color.png"
  },
  "app": {
     "background": {
       "scripts": ["runtime.js"]
     }
  }
}
